  "testingScenarios": [
    {
      "id": "SCENARIO_001",
      "name": "Complete Farm-to-Consumer Journey",
      "description": "Test complete traceability from collection to consumer QR scan",
      "steps": [
        "Farmer collects Ashwagandha roots using mobile app",
        "GPS coordinates and environmental data recorded",
        "Batch transported to processing facility",
        "Quality testing at NABL certified laboratory",
        "Processing into powder/tablets",
        "Final product packaging with QR codes",
        "Consumer scans QR code to view complete provenance"
      ],
      "expectedDuration": "30 days",
      "successCriteria": [
        "All data points captured accurately",
        "QR code provides complete traceability",
        "Blockchain transactions confirmed",
        "Quality parameters within specifications"
      ]
    },
    {
      "id": "SCENARIO_002",
      "name": "Quality Gate Validation",
      "description": "Test quality gate enforcement and rejection handling",
      "steps": [
        "Submit batch with withanolides below minimum threshold",
        "System should reject batch automatically",
        "Farmer receives notification of rejection",
        "Corrective action taken",
        "Re-testing and approval process"
      ],
      "expectedDuration": "7 days",
      "successCriteria": [
        "Quality gates enforced correctly",
        "Rejection notifications sent",
        "Audit trail maintained",
        "Re-testing process works"
      ]
    },
    {
      "id": "SCENARIO_003",
      "name": "Geo-fencing Compliance",
      "description": "Test geo-fencing rules for approved harvesting zones",
      "steps": [
        "Attempt collection outside approved zone",
        "System should prevent submission",
        "Farmer receives geo-fencing violation alert",
        "Collection from approved zone succeeds"
      ],
      "expectedDuration": "2 days",
      "successCriteria": [
        "Geo-fencing rules enforced",
        "Violations detected and blocked",
        "Approved zones work correctly",
        "GPS accuracy within 5 meters"
      ]
    },
    {
      "id": "SCENARIO_004",
      "name": "Offline Functionality",
      "description": "Test offline data capture and synchronization",
      "steps": [
        "Farmer works in area with poor connectivity",
        "Data captured and stored locally",
        "Device comes back online",
        "Data automatically syncs to blockchain",
        "Verify data integrity"
      ],
      "expectedDuration": "5 days",
      "successCriteria": [
        "Offline capture works reliably",
        "Data syncs when online",
        "No data loss occurs",
        "Timestamps preserved correctly"
      ]
    }
  ],
