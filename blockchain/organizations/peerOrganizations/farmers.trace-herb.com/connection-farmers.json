{
    "version":  "1.0.0",
    "name":  "farmers-trace-herb-network",
    "organizations":  {
                          "Farmers":  {
                                          "peers":  [
                                                        "peer0.farmers.trace-herb.com"
                                                    ],
                                          "certificateAuthorities":  [
                                                                         "ca.farmers.trace-herb.com"
                                                                     ],
                                          "mspid":  "FarmersMSP"
                                      }
                      },
    "client":  {
                   "connection":  {
                                      "timeout":  {
